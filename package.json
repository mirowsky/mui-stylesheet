{
  "name": "@mirowsky/mui-stylesheet",
  "version": "1.0.5",
  "description": "Factory function to generate statically typed @mui/system theme aware React Native-esque stylesheet for component scoped styling and better code structuring/organization",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "module": "dist/index.esm.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "node ./.esbuild/core.module.config.js & tsc & node ./.esbuild/core.bundle.config.js",
    "build:scripts": "node ./.esbuild/scripts.config.js & tsc",
    "preinstall": "npx only-allow pnpm"
  },
  "keywords": ["@mui/system", "@mui/material"],
  "author": "Aleksander Mirowsky",
  "license": "MIT",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.2.5",
    "@mui/system": "^5.2.5",
    "@swc/cli": "^0.1.55",
    "@swc/core": "^1.2.121",
    "@swc/jest": "^0.2.15",
    "@types/jest": "^27.0.3",
    "@types/node": "^17.0.2",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "esbuild": "^0.14.7",
    "eslint": "^8.5.0",
    "jest": "^27.4.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^4.5.4"
  },
  "peerDependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/system": "^5.2.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  }
}
